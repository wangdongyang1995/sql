<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sql.mapper.TeacherMapper">
    <select id="select" resultType="com.example.sql.entity.Teacher">
        select  * from teacher

    </select>


    <select id="sele" resultType="com.example.sql.entity.Teacher">
        select  * from teacher where id =#{id}

    </select>


</mapper>